{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        // "module": "es2022",
        "strict": true,
        "paths": {
            "@/*": ["./*"],
            "tailwind.config": ["./tailwind.config.js"]
        }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts",
        "nativewind-env.d.ts"
    ],

    "exclude": ["node_modules"]
}
