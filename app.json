{
    "expo": {
        "name": "running-journal-rn",
        "slug": "running-journal-rn",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "assets/images/shoe-colorful-placeholder.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "assets/images/shoe-colorful-placeholder.png",
                "backgroundColor": "#ffffff"
            },

            "package": "com.anonymous.runningjournalrn"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "assets/images/shoe-colorful-placeholder.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            "expo-sqlite"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "0c5c7386-2bcf-42ba-af9f-f9c49e69eb7d"
            }
        }
    }
}
